<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bjtu.dao.ScDao">


    <select id="findCoursesBySno" resultType="java.lang.Integer">
        SELECT cno
        FROM sc
        WHERE sno = #{sno}
    </select>

    <select id="getFinalScoresBySno" resultType="java.util.Map">
        SELECT cno,score
        FROM sc
        WHERE sno = #{sno}
    </select>
</mapper>