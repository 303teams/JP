<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bjtu.dao.SimilarityDao">


    <insert id="addOne">
        insert into similarity(contentID1,contentID2,similar)
        values (#{contentID1}, #{contentID2},#{similar})
    </insert>

</mapper>